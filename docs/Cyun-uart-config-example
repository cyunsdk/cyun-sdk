	option secturl 'https://iot-auth.cn-shanghai.aliyuncs.com/auth/register/device'
	option curpdt 'qyunsdk'
	option partner 'qlnk'
	option shadowint '60000'
	option debughex '0xffffffff'

config product qyunsdk
	option deviceid 'sdkpc'
	option devicesecrt 'yCVxhARrz6vaioYRwyQ9fSGNBtkZTWKI'
	option productkey 'a1gMUMcUyDP'
	option productsecrt '7ktmqKpl8wy4L1eX'

config uart usb0
	option enabled 1
	option uartname '/dev/ttyUSB0'
	option mode '8N10'
	option bandrate '115200'
